  box-sizing: border-box;
}

.heeader {
  width: 100%;
  height: 100%;
  background: #fff;
}

.all {
  width: 100%;
  height: 230px;
  margin-bottom: 10px;
}

.content {
  width: 100%;
  height: 225px;
  text-align: center;
  padding: 15px 0px;
  box-sizing: border-box;
  background: url("https://yanghongapp.oss-cn-qingdao.aliyuncs.com/UI/%E8%83%8C%E6%99%AF.png") no-repeat;
  /* background-position: 20px 70px; */
  background-size: 100% 255px;

}

.left {
  float: left;
  padding-left: 20px;
  font-size: 12px;
  color: #8b8b8b;
  margin-bottom: 10px;
  width: 100%;
  text-align: left;
  box-sizing: border-box;
}

.content-text {
  display: inline-block;
  width: 91%;
  height: 35px;
  margin-left: 10px;
  text-align: left;
  font-size: 14px;
  /* color: #8b8b8b; *//* border-bottom: 2px solid #E5E3E3; */
}

.title-images {
  width: 100px;
  height: 100px;
  margin: 10px auto;
}

.studyPeople {
  width: 100%;
  height: 435px;
  padding-top: 15px;
  border-top: 5px solid #f1f2f7;
  border-bottom: 5px solid #f1f2f7;
}

.first-text {
  width: 100%;
  position: relative;
}

.first-content {
  width: 80%;
  background: #f1f2f7;
  border-radius: 9px;
  padding: 7px;
  margin-left: 20px;
  margin-top: 35px;
  font-size: 12px;
  box-sizing: border-box;
}

.first-img {
  width: 30px;
  height: 30px;
  position: absolute;
  right: 6%;
  top: 90%;
}

